# Database Configuration
CONNECTION_STRING=mongodb://127.0.0.1:27017/your_database_name

# JWT Secrets (generate new ones)
JWT_SECRET=your_jwt_secret_here
ADMIN_ACCESS_TOKEN_SECRET=your_admin_secret_here
EMPLOYEE_ACCESS_TOKEN_SECRET=your_employee_secret_here

# AWS Configuration (use new credentials)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=your_aws_region
S3_BUCKET_NAME=your_s3_bucket_name

# Payment Gateway (use new credentials)
RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_secret

PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_secret

# Email Configuration
ACCOUNTS_EMAIL=your_accounts_email
EMPLOYEE_MAIL=your_employee_email
SUPPORT_MAIL=your_support_email

# Other Configuration
MODE=development
DISPLAY=:100